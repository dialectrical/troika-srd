{
  "name": "@sanity/uuid",
  "version": "3.0.1",
  "description": "Simple generation of sanity document IDs",
  "sideEffects": false,
  "main": "./lib/cjs/uuid.js",
  "module": "./lib/esm/uuid.js",
  "types": "./lib/esm/uuid.d.ts",
  "keywords": [
    "sanity",
    "cms",
    "headless",
    "realtime",
    "content",
    "uuid"
  ],
  "author": "Sanity.io <hello@sanity.io>",
  "license": "MIT",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc --module es6 --outDir lib/esm",
    "build:cjs": "tsc --module commonjs --outDir lib/cjs",
    "prepublishOnly": "npm run build",
    "clean": "rimraf lib",
    "type-check": "tsc --noEmit",
    "test": "npm run build:cjs && node test/uuid.test.js"
  },
  "dependencies": {
    "@types/uuid": "^8.0.0",
    "uuid": "^8.0.0"
  },
  "bugs": {
    "url": "https://github.com/sanity-io/sanity/issues"
  },
  "homepage": "https://www.sanity.io/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanity-io/sanity.git"
  },
  "devDependencies": {
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.0.5"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100,
    "bracketSpacing": false,
    "singleQuote": true
  }
}
