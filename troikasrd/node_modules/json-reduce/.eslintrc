{
  "env": {
    "node": true
  },
  "parser": "babel-eslint",
  "extends": [
    "standard",
    "prettier",
    "prettier/flowtype"
  ],
  "plugins": [
    "prettier",
    "flowtype"
  ],
  "rules": {
    "prettier/prettier": ["error", {"semi": false, "printWidth": 80, "bracketSpacing": false, "singleQuote": true}]
  }
}
