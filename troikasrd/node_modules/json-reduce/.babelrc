{
  "presets": [
    ["env", {
      "targets": {
        "node": 4,
        "browsers": [
          "last 2 versions",
          "> 5%"
        ]
      }
    }]
  ],
  "plugins": [
    "syntax-object-rest-spread",
    "transform-object-rest-spread",
    "syntax-class-properties",
    "transform-class-properties",
    "transform-flow-comments"
  ]
}
